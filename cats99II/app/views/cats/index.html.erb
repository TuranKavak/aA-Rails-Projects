<section class="cat-index-holder">
  <nav>
    <h1>ALL DEM BABY CATS</h1>
    <a href="<%= new_cat_url %>">
      Add Cat
      <i class="fa fa-plus" aria-hidden="true"></i>
    </a>
  </nav>
  <section class="cat-index">
    <% @cats.each do |cat| %>
    <div class="cat-index-item">
      <div>
        <img src="http://www.chloesplace.co.uk/kitten-01.jpg">
        <br /><a href="<%= cat_url(cat) %>">Rent <%= cat.name %></a>
      </div>
    </div>
    <% end %>
  </section>
</section>


<p>Other content for the cats index goes here</p>

<% content_for :footer do %>
<p>This is the cats index page.</p>
<% end %>