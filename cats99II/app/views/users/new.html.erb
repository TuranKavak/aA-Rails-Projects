<h1>New User</h1>

<%= render 'shared/errors' %>

<form action="<%= users_url %>" method="post">
  
<%= auth_token %>

  <label for="user_username">Username</label>
  <input
     id="user_username"
     name="user[username]"
     type="text"
     value="<%= @user.username %>">
  <br>

  <label for="user_password">Password</label>
  <input
     id="user_password"
     name="user[password]"
     type="password"
     value="<%= @user.password %>">
  <br>

  <input type="submit">
</form>